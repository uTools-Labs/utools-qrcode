{
  "main": "index.html",
  "logo": "logo.png",
  "pluginSetting": {
    "single": false
  },
  "features": [
    {
      "code": "QRCode",
      "explain": "二维码生成及解码",
      "cmds": [
        "QRCode",
        "二维码",
        {
          "type": "regex",
          "match": "/^https?:\\/\\/(?:(?:(?:25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]\\d|\\d)(?:\\.(?:25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]\\d|\\d)){3})|(?:(?:[a-z\\u00a1-\\uffff0-9]-*)*[a-z\\u00a1-\\uffff0-9]+)(?:\\.(?:[a-z\\u00a1-\\uffff0-9]-*)*[a-z\\u00a1-\\uffff0-9]+)*(?:\\.(?:[a-z\\u00a1-\\uffff]{2,}))\\.?)(?::\\d{2,5})?(?:[/?#][^\\s\"]*)?$/i",
          "label": "生成二维码"
        },
        {
          "type": "img",
          "label": "解码二维码"
        },
        {
          "type": "window",
          "platform": ["win32", "darwin"],
          "match": {
            "app": ["chrome.exe", "firefox.exe", "MicrosoftEdge.exe", "iexplore.exe", "opera.exe", "brave.exe", "msedge.exe", "Google Chrome.app", "Safari.app", "Opera.app", "Vivaldi.app", "Brave Browser.app", "Microsoft Edge.app"]
          },
          "label": "网页二维码"
        }
      ]
    }
  ]
}